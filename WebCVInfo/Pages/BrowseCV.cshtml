@page
@model WebCVInfo.Pages.BrowseCVModel
@{
}
<h1 style="color:aqua">Welcome to Browse Services</h1>

<table style="border:solid 1px aqua;padding:10px;width:auto">

    <tr>
        <th style="border:solid 1px aqua;padding:10px">Id</th>
        <th style="border:solid 1px aqua;padding:10px">Name</th>
        <th style="border:solid 1px aqua;padding:10px">Gender</th>
        <th style="border:solid 1px aqua;padding:10px">View</th>
    </tr>

    @foreach(var intern in Model.interns)
    {
        <tr>
            <td style="border:solid 1px aqua;padding:10px">@intern.InternID</td>
            <td style="border:solid 1px aqua;padding:10px">@intern.InternName</td>
            <td style="border:solid 1px aqua;padding:10px">@intern.Gender</td>
            <td style="border:solid 1px aqua;padding:10px"><a asp-page="details" asp-route-name="@intern.InternName">Details</a>
                |<a asp-page="Edit" asp-route-id="@intern.InternID" asp-route-name="@intern.InternName"> Edit </a> 
                | <a asp-page="DeleteRecord" asp-route-id="@intern.InternID">Delete</a> </td>
        </tr>
    }
</table>


